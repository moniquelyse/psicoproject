{
  "rewrites": [
    {
      "source": "/blog",
      "destination": "/blog/dist/index.html"
    },
    {
      "source": "/blog/:path*",
      "destination": "/blog/dist/:path*"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "zeroConfig": true
      }
    },
    {
      "src": "blog/package.json",
      "use": "@vercel/static-build",
      "config": {
        "zeroConfig": true
      }
    }
  ],
  "buildCommand": "npm run build && cd blog && npm install && npm run build",
  "outputDirectory": "dist"
}